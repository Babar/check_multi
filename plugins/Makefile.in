prefix=@prefix@
LIBEXECDIR=@LIBEXECDIR@
HTMLDIR=@datarootdir@
INSTALL=@INSTALL@
INSTALL_OPTS=@INSTALL_OPTS@

all:


clean:
	-rm -f check_multi

distclean: clean
	-rm -f Makefile

devclean: distclean

install: 
	$(INSTALL) -m 755 $(INSTALL_OPTS) -d $(DESTDIR)$(LIBEXECDIR)
	$(INSTALL) -m 755 $(INSTALL_OPTS) check_multi $(DESTDIR)$(LIBEXECDIR)

