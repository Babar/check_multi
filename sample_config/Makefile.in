prefix=@prefix@
LIBEXECDIR=@libexecdir@
HTMLDIR=@datarootdir@
INSTALL=@INSTALL@
INSTALL_OPTS=@INSTALL_OPTS@

all: 
	echo "make install-config clean distclean"

clean:
	cd cluster		&& $(MAKE) $@
	cd feed_passive		&& $(MAKE) $@
	cd multi_long		&& $(MAKE) $@
	cd network_interface	&& $(MAKE) $@
	cd nagiostats		&& $(MAKE) $@
	cd start_process	&& $(MAKE) $@
	

distclean: clean
	cd cluster		&& $(MAKE) $@
	cd feed_passive		&& $(MAKE) $@
	cd multi_long		&& $(MAKE) $@
	cd network_interface	&& $(MAKE) $@
	cd nagiostats		&& $(MAKE) $@
	cd start_process	&& $(MAKE) $@
	-rm -f Makefile

devclean: distclean

install-config: 
	cd cluster		&& $(MAKE) $@
	cd feed_passive		&& $(MAKE) $@
	cd multi_long		&& $(MAKE) $@
	cd network_interface	&& $(MAKE) $@
	cd nagiostats		&& $(MAKE) $@
	cd start_process	&& $(MAKE) $@
